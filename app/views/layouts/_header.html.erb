<header class="header">
	<%= link_to root_path do %>
		<strong class="header-title">Fashion Flee</strong>
	<% end %>
	<% if signed_in? %>
		<div>
			<%= link_to "My page", user_path(current_user) %>
			<%= link_to "Search", items_path %>
			<%= link_to "Log out", destroy_user_session_path, data: { confirm: "Sign out ?" }, method: :delete %>
		</div>
	<% else %>
		<div>
			<%= link_to "Sign up", new_user_registration_path %>
			<%= link_to "Log in", new_user_session_path %>
		</div>
	<% end %>
</header>
<% if signed_in? %>
	Signed in as <%= current_user.name %><br>
<% end %>