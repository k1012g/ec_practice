<header class="header">
	<%= link_to root_path do %>
		<strong class="header-title">Fashion Flee</strong>
	<% end %>
	<% if signed_in? %>
		<div>
			<strong>
				<%= link_to "Myページ", user_path(current_user) %>
				<%= link_to "商品一覧", items_path %>
				<%= link_to "ログアウト", destroy_user_session_path, data: { confirm: "Sign out ?" }, method: :delete %>
			</strong>
		</div>
	<% else %>
		<div>
			<strong>
				<%= link_to "会員登録", new_user_registration_path %>
				<%= link_to "ログイン", new_user_session_path %>
			</strong>
		</div>
	<% end %>
</header>
<% if signed_in? %>
	<strong>
		Signed in as <%= current_user.name %><br>
	</strong>
<% end %>